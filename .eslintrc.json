{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
